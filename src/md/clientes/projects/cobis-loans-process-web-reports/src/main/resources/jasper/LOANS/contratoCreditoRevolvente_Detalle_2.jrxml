			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement stretchType="RelativeToBandHeight" x="34" y="0" width="473" height="12" isPrintWhenDetailOverflows="true" forecolor="#000000" uuid="dbe873f1-69c0-485e-8e74-06973f0a49e2"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="SansSerif" size="7" isBold="false"/>
					<paragraph leftIndent="2" rightIndent="2"/>
				</textElement>
				<textFieldExpression><![CDATA["Número de Identificación Personal NIP."]]></textFieldExpression>
			</textField>
