		<class>com.cobiscorp.ecobis.fpm.portfolio.model.ItemsPortfolio</class>
		<class>com.cobiscorp.ecobis.fpm.portfolio.model.Operation</class>
		<class>com.cobiscorp.ecobis.fpm.portfolio.model.OperationTemp</class>
		<class>com.cobiscorp.ecobis.fpm.portfolio.model.ReferencialValue</class>
		<class>com.cobiscorp.ecobis.fpm.portfolio.model.ValueDetailPortfolio</class>
		<class>com.cobiscorp.ecobis.fpm.portfolio.model.ValuePortfolio</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
