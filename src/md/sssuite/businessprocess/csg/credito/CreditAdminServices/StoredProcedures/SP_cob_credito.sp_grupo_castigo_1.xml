        <column name="NU_TRAMITE" />
      </columns>
      <queryText>SELECT  gc_fecha_corte,sindico1,sindico2 </queryText>
    </query>
    <query>
      <columns>
        <column name="FECHA_CORTE" />
        <column name="SINDICO1" />
        <column name="SINDICO2" />
      </columns>
      <queryText></queryText>
    </query>
    <query>
      <columns>
        <column name="Oficina" />
        <column name="Moneda" />
        <column name="TotalCap" />
        <column name="TotalCapFecha" />
      </columns>
      <queryText></queryText>
    </query>
