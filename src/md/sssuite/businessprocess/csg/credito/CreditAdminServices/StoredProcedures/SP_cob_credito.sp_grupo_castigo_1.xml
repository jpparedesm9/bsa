      <queryText>SELECT "GRUPO"=ctx.ISNULL(i_grupo, 0),"PADRE"=ctx.ISNULL(w_padre, 0),"ESTADO"=ctx.ISNULL(w_estado, "X"),"FECHA"=w_fecha_corte</queryText>
    </query>
    <query>
      <columns>
        <column name="Codigo" />
        <column name="Banco" />
